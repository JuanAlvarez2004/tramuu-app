    "test:integration:inventory": "jest --config jest.integration.config.js inventory.service.integration",
    "test:integration:deliveries": "jest --config jest.integration.config.js deliveries.service.integration",
    "test:integration:employees": "jest --config jest.integration.config.js employees.service.integration",
    "test:integration:companies": "jest --config jest.integration.config.js companies.service.integration",
    "test:all": "npm run test && npm run test:integration"
