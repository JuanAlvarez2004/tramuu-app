{
  "expo": {
    "name": "Tramuu",
    "slug": "tramuu-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tramuu-logo.webp",
    "scheme": "tramuuapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tramuu.app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.tramuu.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/tramuu-logo.webp",
        "backgroundImage": "./assets/images/tramuu-logo.webp",
        "monochromeImage": "./assets/images/tramuu-logo.webp"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/tramuu-logo.webp"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicaci칩n necesita acceso a tus fotos para guardar im치genes de las pruebas de calidad.",
          "cameraPermission": "La aplicaci칩n necesita acceso a tu c치mara para tomar fotos de las pruebas de calidad."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      }
    }
  }
}
